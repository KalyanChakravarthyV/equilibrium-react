{
  "name": "equilibrium",
  "homepage": "/html/en/default/rest/Equilibrium/app/main/home.html",
  "version": "1.0.1",
  "private": true,
  "repository": "git@github.com:KalyanChakravarthyV/equilibrium-react.git",
  "bugs": "https://github.com/KalyanChakravarthyV/equilibrium-react/issues",
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -f main.zip && next build && zip -r main.zip ./main",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "ci-check": "yarn format:diff",
    "clean": "yarn cache clean && yarn install ",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{js,md,scss}\"",
    "format:diff": "prettier --list-different \"**/*.{js,md,scss}\"",
    "format:staged": "prettier --write",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@carbon/pictograms-react": "^11.50.0",
    "@carbon/react": "^1.35.0",
    "@codemirror/lang-javascript": "6.0.1",
    "@codemirror/lang-sql": "^6.5.4",
    "@octokit/core": "4.2.0",
    "@uiw/react-codemirror": "^4.23.5",
    "apollo-boost": "0.4.2",
    "codemirror": "^6.0.1",
    "dotenv": "^16.4.5",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "file-saver": "^2.0.5",
    "graphql": "14.3.1",
    "next": "13.4.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.66.1",
    "zipyai": "^1.7.33"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "sass": "1.29.0",
    "wait-for-expect": "3.0.2"
  },
  
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn format:staged",
      "git add"
    ],
    "*.{scss,css}": [
      "yarn format:staged",
      "git add"
    ],
    "*.md": [
      "yarn format:staged",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
